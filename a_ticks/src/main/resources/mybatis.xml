<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings>
	    <!-- 启用懒加载，必须按照下面方式配置 -->
		<setting name="lazyLoadingEnabled" value="true"/>
		<!-- 将积极加载改为消极加载即按需加载。必须写,且为false才会懒加载 -->  
		<setting name="aggressiveLazyLoading" value="false"/>
		<!-- 开启二级缓存 -->
		<setting name="cacheEnabled" value="true"/>
	</settings>

	<!-- 设置类的别名 -->
	<typeAliases>
		<!-- <typeAlias type="com.wxb.entity.User" alias="User"/>  -->
		<typeAlias type="com.wxb.entity.Authority" alias="Authority"/> 
		<typeAlias type="com.wxb.entity.LoginLog" alias="LoginLog"/> 
		<!-- <typeAlias type="com.wxb.entity.Student" alias="Student"/>  -->
		<!-- <typeAlias type="com.wxb.entity.Staff" alias="Staff"/>  -->
		
	</typeAliases>

	<!-- <plugins>
	<plugin interceptor="com.chrhc.mybatis.autodate.interceptor.AutoDateInterceptor">
